{
  "name": "ts-frontend-rust",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "parcel serve src/index.html --open",
    "format": "cargo fmt",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "test": "run-s test:*",
    "test:rust": "cargo test",
    "test:typescript": "jest",
    "build": "parcel build src/index.html --public-url .",
    "predeploy": "run-s format lint test build type-check",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "gh-pages": "^3.1.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-wasm.rs": "^1.2.16",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
